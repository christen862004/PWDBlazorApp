<div style="border:5px solid red;margin:10px">
    <h3>Child</h3>
    <h3>@Msg</h3>
    <h2>@no</h2>
</div>

@code {
    [CascadingParameter(Name = "Message")]
    public string Msg { get; set; } = "default message from Child";//[parmeter]

    [CascadingParameter(Name = "no")]
    public int no { get; set; } = 100;
}
